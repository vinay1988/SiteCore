@model events.tac.local.Models.OverviewList


<ul class="block-grid-md-4 block-grid-sm-3 block-grid-xs-1">
    @foreach(var x in Model)
    { 
    <li>
        <div class="thumbnail">
            <a href="@x.Url">
               @x.Image
            </a>
            <div class="caption">
                <h3 class="teaser-heading">@x.Title</h3>
                <a href="@x.Url" class="btn btn-default">Read more</a>
            </div>
        </div>
    </li>
    }
</ul>
